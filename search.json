---
layout: null
---
[
  {% for item in site.data.list %}
    {

      "title"     : "{{ item.title }}",
      "url"       : "{{ site.baseurl }}booklist#{{ item.bookshelf }}",
      "author"    : "{{ item.author }}",
      "year"      : "{{ item.year }}",
      "no"        : "{{ item.No }}",
      "bookshelf" : "{{ item.notes }}",
      "publisher" : "{{ item.publisher }}",
      "page"      : "{{ item.page }}",
      "uniqueid"  : "{{ item.bookshelf }}",
      "status"    : "{{ item.status }}",
      "reread"    : "{{ item.reread }}"

    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]