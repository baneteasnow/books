---
layout: null
---

{% capture json %}
[
  {% for item in site.data.list %}
    {

      "title"     : "{{ item.title }}",
      "url"       : "{{ site.baseurl }}bookstoc#{{ item.title }}",
      "author"    : "{{ item.author }}",
      "year"      : "{{ item.year }}",
      "no"        : "{{ item.No }}",
      "bookshelf" : "{{ item.notes }}",
      "uniqueid"  : "{{ item.bookshelf }}"

    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]
{% endcapture %}

{{ json | lstrip }}
